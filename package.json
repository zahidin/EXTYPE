    "@types/request-promise": "^4.1.45",
    "@types/supertest": "^2.0.8",
    "@types/validator": "^10.11.3",
    "bcrypt": "^5.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
